{
  "name": "@awcp/sdk",
  "version": "0.1.0",
  "description": "AWCP SDK - Host and Remote Daemon implementations",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./host": {
      "types": "./dist/host/index.d.ts",
      "import": "./dist/host/index.js"
    },
    "./remote": {
      "types": "./dist/remote/index.d.ts",
      "import": "./dist/remote/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rm -rf dist *.tsbuildinfo",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@awcp/core": "^0.1.0"
  },
  "peerDependencies": {
    "@a2a-js/sdk": "^0.3.0"
  },
  "peerDependenciesMeta": {
    "@a2a-js/sdk": {
      "optional": true
    }
  },
  "keywords": [
    "awcp",
    "agent",
    "sdk",
    "host",
    "remote"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/anthropics/awcp.git",
    "directory": "packages/sdk"
  }
}
